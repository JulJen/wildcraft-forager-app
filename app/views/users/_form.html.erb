<%= simple_form_for @user, url: signup_path  do |f| %>

  <%= f.error_notification %>

  <br><br>

  <%= f.input :name %>

  <%= f.input :email, input_html: { autocomplete: 'email' } %>

  <%= f.input :password, input_html: { autocomplete: 'current-password' } %>

  <%= f.submit 'Create Account' %>

<% end %>
